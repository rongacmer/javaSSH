<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ssh.x.entity">
	<class name="Selectcourse" table="Selectcourse">
	<composite-id> 
            <key-property name="cno"  column="cno" type="java.lang.String"/> 
            <key-property name="sno" column="sno"  type="java.lang.String"/>
	</composite-id>
	<property name="score" column="score" type="java.lang.Integer"/>
	</class>
</hibernate-mapping>
