<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ssh.x.entity">
	<class name="Teacher" table="tea">
		<id name="Tno">
			<generator class="assigned">
			</generator>
		</id>
		<property name="phone" />
		<property name="TSex" not-null="true"/>
		<property name="Tname" not-null="true"/>
	</class>
</hibernate-mapping>
