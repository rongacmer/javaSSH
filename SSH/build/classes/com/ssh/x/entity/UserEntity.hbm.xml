<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ssh.x.entity">
	<class name="UserEntity" table="user_">
		<id name="userName" column="id">
			<generator class="assigned">
			</generator>
		</id>
		<property name="userPass" column="Password" not-null="true"/>
		<property name="role" not-null="true"/>
	</class>
</hibernate-mapping>
